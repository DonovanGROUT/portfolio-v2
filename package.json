{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:a11y": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:run:threads": "VITEST_POOL_THREADS=4 vitest run",
    "test:run:threads:verbose": "VITEST_POOL_THREADS=4 vitest run --reporter=verbose",
    "test:coverage:threads": "VITEST_POOL_THREADS=4 vitest run --coverage",
    "test:coverage:threads:verbose": "VITEST_POOL_THREADS=4 vitest run --coverage --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "test:coverage:strict": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run format:check && npm run lint:strict && npm run test:run:threads",
    "prepare": "husky",
    "postbuild": "npx next-sitemap",
    "lighthouse": "bash scripts/check-server.sh && lhci autorun",
    "lighthouse:mobile": "bash scripts/check-server.sh && lhci autorun --config=.lighthouserc.mobile.json",
    "audit:security": "audit-ci --config audit-ci.json"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "next": "16.0.10",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.5",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.3",
    "audit-ci": "^7.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.5.3",
    "typescript": "^5",
    "vitest": "^3.2.3",
    "wait-on": "^8.0.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
